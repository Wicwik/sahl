parameters:


php:
	date.timezone: Europe/Prague


nette:
	application:
		errorPresenter: Error
		mapping:
			*: App\*Module\Presenters\*Presenter

	session:
		expiration: 14 days

	security:
		users:
			admin:	secret	#	users'admin',	passwords'secret'
			user:	secret	#	users'user',	passwords'user'
			alexander.valach:	wallach789	#	users'user',	passwords'user'
			sahl:	sahl	#	users'user',	passwords'user'

services:
	- App\RouterFactory
	router: @App\RouterFactory::createRouter
	- App\Model\PostsRepository
	- App\Model\PlayersRepository
	- App\Model\TeamsRepository
	- App\Model\EventsRepository
	- App\Model\UsersRepository
	- App\Model\FightsRepository
	- App\Model\ForumRepository
	- App\Model\LinksRepository
	- App\Model\PostImageRepository
	- App\Model\RulesRepository
	- App\Model\TablesRepository
	- App\Model\GalleryRepository
	- App\Model\AlbumsRepository
	- App\Model\RoundsRepository
	- App\Model\PunishmentsRepository
	- App\Model\GoalsRepository
	- App\Model\ReplyRepository
